{
    "version": 2,
    "builds": [
        {
            "src": "config/sistema/express.js",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "src": "(.*)",
            "dest": "config/sistema/server.js"
        },
        {
            "src": "/teste",
            "dest": "config/sistema/consulta.html"
        }
    ]
}